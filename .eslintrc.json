{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true
  },
  "rules": {
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "off",
    "no-unreachable": "warn",
    "no-unused-vars": "off",
    "constructor-super": "warn",
    "valid-typeof": "warn",
    "comma-dangle": "off",
    "prefer-arrow-callback": "off",
    "linebreak-style": "off",
    "func-names": "off"
  },
  "globals": {
    "Materialize": true,
    "intervalID": true,
    "jQuery": true,
    "$": true,
    "window": true,
    "navigator": true
  }
}
